      actionType: 4
      coords: {x: 0, y: 0}
      moveSpeed: 0
    - speaker: {fileID: 11400000, guid: d98b35069e62905439c4e8bfca3822c3, type: 2}
      spriteIndex: 0
      actionType: 5
      coords: {x: 0, y: 0}
      moveSpeed: 0
  - text: Humpf...
