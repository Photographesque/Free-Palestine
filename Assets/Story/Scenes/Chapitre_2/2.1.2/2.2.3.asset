      spriteIndex: 0
      actionType: 4
      coords: {x: 724, y: -155}
      moveSpeed: 0
  - text: "Tr\xE8s dr\xF4le... Ah oui ? "
    speaker: {fileID: 11400000, guid: eddfe009b49d3484d8dfb5498d6a2f17, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 2f1d8a3ec257bb04b8ad7bff157dc550, type: 2}
